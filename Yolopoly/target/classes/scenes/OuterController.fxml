            </ImageView>
            <ImageView id="ok_but_green" fitHeight="100.0" fitWidth="250.0" layoutX="980.0" layoutY="510.0" onMouseClicked="#close_settings" pickOnBounds="true" preserveRatio="true" styleClass="shadowed">
