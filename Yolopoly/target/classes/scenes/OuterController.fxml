            </ImageView>
            <ImageView id="quit-no" fitHeight="100.0" fitWidth="250.0" layoutX="980.0" layoutY="510.0" onMouseClicked="#quitGameNoPressed" pickOnBounds="true" preserveRatio="true" styleClass="shadowed">
