         <ImageView id="feedback" fitHeight="100.0" fitWidth="250.0" layoutX="695.0" layoutY="755.0" onMouseClicked="#coming_soon" pickOnBounds="true" preserveRatio="true" styleClass="shadowed">
            <image>
               <Image url="@sources/outer-controller/feedback.png" />
            </image>
         </ImageView>
         <ImageView id="credits" fitHeight="100.0" fitWidth="250.0" layoutX="975.0" layoutY="755.0" onMouseClicked="#coming_soon" pickOnBounds="true" preserveRatio="true" styleClass="shadowed">
            <image>
               <Image url="@sources/outer-controller/credits.png" />
            </image>
         </ImageView>
