            <Label layoutX="43.0" layoutY="103.0" text="Label" />
            <ImageView id="ok" fitHeight="100.0" fitWidth="250.0" layoutX="690.0" layoutY="510.0" onMouseClicked="#close_settings" pickOnBounds="true" preserveRatio="true" styleClass="shadowed">
               <image>
